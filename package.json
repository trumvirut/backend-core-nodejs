        "backend seed 2",
        "development environment",
        "nodejs",
        "typescript",
        "expressjs",
        "socket.io",
        "typeorm",
        "postgresql",
        "redis",
        "eslint",
        "mocha",
        "nyc",
        "unit test",
        "coverage",
        "caching",
        "multi-layer",
        "singleton",
        "generate module",
        "migration"
    ],
