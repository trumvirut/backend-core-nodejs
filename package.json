    "description": "Backend environment 2",
    "license": "MIT",
    "readmeFilename": "README.md",
    "scripts": {
        "cache:clear": "./node_modules/.bin/ts-node ./node_modules/typeorm/cli.js cache:clear",
        "migration:generate": "NODE_ENV=development ./node_modules/.bin/ts-node ./node_modules/typeorm/cli.js migration:generate",
        "migration:run": "NODE_ENV=development ./node_modules/.bin/ts-node ./node_modules/typeorm/cli.js migration:run",
        "migration:revert": "NODE_ENV=development ./node_modules/.bin/ts-node ./node_modules/typeorm/cli.js migration:revert",
        "generate:module": "node ./src/resources/templates/source/loader.js",
        "lint": "./node_modules/.bin/eslint --ext .ts --ext .js ./src",
        "test": "npm run build && NODE_ENV=test ./node_modules/.bin/nyc ./node_modules/.bin/mocha --exit --require ./dist/test/**/*.js",
        "build": "npm run lint && ./node_modules/.bin/grunt clean && ./node_modules/.bin/tsc && ./node_modules/.bin/grunt sync",
        "dev": "NODE_ENV=development ./node_modules/.bin/nodemon --delay 0.8 --watch src/ --watch .env --exec './node_modules/.bin/ts-node' src/app.ts",
        "start": "node ./dist/app.js",
        "now-build": "npm run build"
    },
    "author": {
        "name": "giggs",
        "email": "giggs@namtech.com.au"
    },
    "repository": {
        "type": "git"
    },
    "keywords": [
